programa {
  funcao inicio() {
    //Declaração de variáveis.
    real a,b

    //Definição de variáveis.
    escreva("Digite aqui suas variáveis: ")
    leia(a, b)
    limpa()
    
    //Condicional.
    se (a > b){
      escreva("a é maior que b")
      }
      senao{
        escreva("b é maior que a")
      }
  }
}
